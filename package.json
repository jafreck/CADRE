{
  "name": "cadre",
  "version": "0.1.0",
  "description": "Coordinated Agent Development Runtime Engine",
  "license": "MIT",
  "author": "Jacob Freck",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "files": [
    "dist/",
    "src/agents/templates/"
  ],
  "bin": {
    "cadre": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "mkdir -p dist/agents && rm -rf dist/agents/templates && cp -r src/agents/templates dist/agents/templates",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:e2e": "CADRE_E2E=1 vitest run tests/e2e-*.test.ts"
  },
  "dependencies": {
    "@cadre/command-diagnostics": "workspace:*",
    "@inquirer/prompts": "^7",
    "@modelcontextprotocol/sdk": "^1.12",
    "@octokit/rest": "^21",
    "chalk": "^5",
    "commander": "^12",
    "date-fns": "^3",
    "ora": "^8",
    "p-limit": "^6",
    "p-queue": "^8",
    "simple-git": "^3.27",
    "zod": "^3.23",
    "zod-to-json-schema": "^3.23"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@vitest/coverage-v8": "^2",
    "tsx": "^4",
    "typescript": "^5.5",
    "vitest": "^2"
  }
}
